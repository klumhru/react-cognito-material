{
  "name": "react-cognito-material",
  "version": "0.2.14",
  "description": "Ties together ReactJS with AWS Cognito identity, using Material UI for styling",
  "license": "MIT",
  "repository": "https://github.com/klumhru/react-cognito-material",
  "main": "build/index.js",
  "dependencies": {
    "amazon-cognito-auth-js": "^1.0.0",
    "amazon-cognito-identity-js": "^1.25.0",
    "aws-sdk": "^2.150.0",
    "material-ui": "^1.0.0-beta.21",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "xregexp": "^3.2.0"
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "node node_modules/eslint/bin/eslint.js src/ -f codeframe"
  },
  "author": {
    "name": "HÃ¶gni Gylfason",
    "email": "klumhru@gmail.com",
    "website": "https://klumhru.com"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": [
          "scripts/**/*.js",
          "config/**/*.js"
        ],
        "excludedFiles": "*.js"
      }
    ],
    "extends": "airbnb",
    "env": {
      "jest": true
    },
    "globals": {
      "document": true,
      "window": true
    },
    "rules": {
      "object-curly-newline": [
        0
      ],
      "no-plusplus": 0,
      "no-console": 0,
      "no-confusing-arrow": [
        0
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "react/forbid-prop-types": [
        0
      ],
      "import/extensions": [
        2,
        {
          "js": "never",
          "jsx": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "semi": [
        2,
        "never"
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "css.escape": "^1.5.1",
    "dotenv": "4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "jss": "^9.3.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.3.0",
    "sinon": "^2.3.2",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  }
}
